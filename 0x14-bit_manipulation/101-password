#include <stdio.h>
#include <string.h>

int main(int argc, char *argv[])
{
   char password[] = "secret";

   if (argc != 2)
   {
       printf("Usage: %s <password>\n", argv[0]);
       return 1;
   }

   if (strcmp(argv[1], password) == 0)
   {
       printf("Congratulations!\n");
       return 0;
   }
   else
   {
       printf("Incorrect password.\n");
       return 1;
   }
}

